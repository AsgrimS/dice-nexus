		"tslib": "^2.4.1",
		"tsx": "^3.12.6",
		"typescript": "^4.9.5",
		"unplugin-icons": "^0.16.1",
		"vite": "^4.2.0",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"dependencies": {
		"@dimforge/rapier3d-compat": "^0.11.2",
		"@floating-ui/dom": "^1.2.7",
		"@prisma/client": "4.13.0",
		"@threlte/core": "6.0.0-next.1",
