generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Game {
  id        Int      @id @default(autoincrement())
  name      String
  createdAt DateTime @default(now())
  players   Player[]
}

model Player {
  id        Int      @id @default(autoincrement())
  name      String
  createdAt DateTime @default(now())
  games     Game[]
}
